{
	"subarray_sum": {
		"prefix": "subarray_sum",
		"body": [
		  "ll findSubarraySum(vector<ll> &vec, ll n, ll sum)",
		  "{",
		  "    ll m=0,cnt=0;",
		  "    map<ll,ll>mp;",
		  "    for(int i=0;i<n;++i)",
		  "    {",
		  "    	m+=vec[i];",
		  "    	if(m==sum)cnt++;",
		  "    	if(mp.count(m-sum))",
		  "    	{",
		  "    		cnt+=mp[m-sum];",
		  "		}",
		  "		mp[m]++;",
		  "	}",
		  "	cout<<cnt<<endl;",
		  "}",
		  " "
		],
		"description": "subarray_sum"
	  }
}